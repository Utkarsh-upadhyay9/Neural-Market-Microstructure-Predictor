# Copy this file to .env and add your actual API keys
# DO NOT commit .env to GitHub

# Alpha Vantage API Key (get from: https://www.alphavantage.co/support/#api-key)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# News API Key (get from: https://newsapi.org/register)
NEWSAPI_KEY=your_newsapi_key_here

# NASDAQ Data Link API Key (get from: https://data.nasdaq.com/sign-up)
NASDAQ_DATA_LINK_KEY=your_nasdaq_key_here

# Database Configuration
DATABASE_PATH=data/market_data.db

# Logging
LOG_LEVEL=INFO
